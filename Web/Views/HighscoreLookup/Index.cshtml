@{
    ViewData["Title"] = "Highscore Lookup";
}

<div class="text-center">
    <h1 class="display-4">Lookup Player</h1>
    <form asp-controller="HighscoreLookup" asp-action="Process">
        <input type="type" name="name" value="" />
        <div class="form-check">
            <input class="form-check-input" type="radio" name="account_type" id="normal" value="" checked>
            <label class="form-check-label" for="exampleRadios1">
                Default radio
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios2" value="option2">
            <label class="form-check-label" for="exampleRadios2">
                Second default radio
            </label>
        </div>

        <input type="submit" name="name" value="" />
    </form>
    
</div>
